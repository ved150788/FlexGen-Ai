{
	"framework": "nextjs",
	"buildCommand": "next build",
	"devCommand": "next dev",
	"outputDirectory": ".next",
	"installCommand": "npm install",
	"functions": {
		"api/python/*.py": {
			"runtime": "python3.9"
		}
	},
	"builds": [
		{
			"src": "next.config.js",
			"use": "@vercel/next"
		},
		{
			"src": "api/python/*.py",
			"use": "@vercel/python"
		}
	],
	"routes": [
		{
			"src": "/api/threat/dashboard",
			"dest": "/api/python/threat-dashboard.py"
		},
		{
			"src": "/api/threat/iocs",
			"dest": "/api/python/threat-iocs.py"
		},
		{
			"src": "/api/threat/search",
			"dest": "/api/python/threat-search.py"
		},
		{
			"src": "/api/threat/taxii-status",
			"dest": "/api/python/taxii-status.py"
		},
		{
			"src": "/(.*)",
			"dest": "/$1"
		}
	]
}
