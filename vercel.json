{
	"framework": "nextjs",
	"buildCommand": "next build",
	"devCommand": "next dev",
	"outputDirectory": ".next",
	"cleanUrls": true,
	"functions": {
		"api/python/**/*.py": {
			"runtime": "vercel-python@3.9",
			"memory": 512,
			"maxDuration": 10
		}
	},
	"installCommand": "pip install -r api/python/requirements.txt && npm install"
}
