{
	"version": 2,
	"builds": [
		{ "src": "api/*.py", "use": "@vercel/python" },
		{ "src": "index.js", "use": "@vercel/node" }
	],
	"routes": [
		{ "src": "/api/contact", "dest": "/api/contact.py" },
		{ "src": "/api/security-audit", "dest": "/api/security-audit.py" },
		{ "src": "/api", "dest": "/api/index.py" },
		{ "src": "/", "dest": "/index.js" }
	]
}
