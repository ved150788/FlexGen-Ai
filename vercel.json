{
	"framework": "nextjs",
	"buildCommand": "next build",
	"devCommand": "next dev",
	"outputDirectory": ".next",
	"installCommand": "npm install",
	"rewrites": [
		{
			"source": "/api/threat/dashboard",
			"destination": "/api/python/threat-dashboard"
		},
		{
			"source": "/api/threat/iocs",
			"destination": "/api/python/threat-iocs"
		},
		{
			"source": "/api/threat/search",
			"destination": "/api/python/threat-search"
		},
		{
			"source": "/api/threat/taxii-status",
			"destination": "/api/python/taxii-status"
		}
	]
}
