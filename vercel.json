{
	"framework": "nextjs",
	"buildCommand": "next build",
	"devCommand": "next dev",
	"outputDirectory": ".next",
	"cleanUrls": true,
	"functions": {
		"api/python/**/*.py": {
			"runtime": "vercel-python@3.9",
			"memory": 512,
			"maxDuration": 10
		}
	},
	"installCommand": "pip install -r requirements.txt && npm install",
	"routes": [
		{ "src": "/api/python/(.*)", "dest": "/api/python/$1" },
		{ "src": "/api/(.*)", "dest": "/api/$1" },
		{ "handle": "filesystem" },
		{ "src": "/(.*)", "dest": "/" }
	]
}
